{
  "name": "psql_bq",
  "type": "pipeline",
  "artifact": {
    "name": "cdap-data-pipeline",
    "version": "6.11.1"
  },
  "config": {
    "stages": [
      {
        "name": "PostgreSQL",
        "plugin": {
          "name": "Postgres",
          "type": "batchsource",
          "properties": {
            "connection": "${conn(postgres_source)}",
            "importQuery": "SELECT * FROM public.order_items"
          }
        }
      },
      {
        "name": "Wrangler",
        "plugin": {
          "name": "Wrangler",
          "type": "transform"
        }
      },
      {
        "name": "BigQuery",
        "plugin": {
          "name": "BigQueryTable",
          "type": "batchsink",
          "properties": {
            "project": "famous-gearing-473807-f4",
            "dataset": "bigquery_ecommerce",
            "table": "users"
          }
        }
      }
    ]
  }
}
